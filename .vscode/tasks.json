{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc build active file",
			"command": "/usr/bin/gcc",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"-Wall",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: /usr/bin/gcc"
		},
		{
			"type": "shell",
			"label": "Escape Pokem√≥n",
			"command": "/usr/bin/gcc -fdiagnostics-color=always -g -O0 -std=c99 -Wall -Wconversion -Wtype-limits -Werror ${file} -o ${fileDirname}/${fileBasenameNoExtension} &&${fileDirname}/${fileBasenameNoExtension} ejemplo/objetos.txt ejemplo/interacciones.txt",
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"detail": "compiler: /usr/bin/gcc"
		}
	]
}